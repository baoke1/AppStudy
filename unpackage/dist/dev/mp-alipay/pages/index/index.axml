<view class="index"><scroll-view class="scroll-content" scroll-x="true" scroll-into-view="{{scorllIndex}}"><view class="scroll-item" id="{{'top'+index}}" a:for="{{topBar}}" a:for-item="item" a:for-index="index" a:key="index"><text data-event-opts="{{[['tap',[['changeTab',[index]]]]]}}" class="{{(topBarIndex===index?'f-active-coo':'f-color')}}" onTap="__e">{{''+item.name}}</text></view></scroll-view><swiper style="{{('height:'+clentHeight+'px;')}}" current="{{topBarIndex}}" data-event-opts="{{[['change',[['OnchangeTab',['$event']]]]]}}" onChange="__e"><swiper-item a:for="{{newTopBar}}" a:for-item="item" a:for-index="index" a:key="index"><scroll-view style="{{('height:'+clentHeight+'px;')}}" scroll-y="true" data-event-opts="{{[['scrollToLower',[['loadMore',[index]]]]]}}" onScrollToLower="__e"><block a:if="{{item.data.length>0}}"><block><block a:for="{{item.data}}" a:for-item="k" a:for-index="i" a:key="i"><block a:if="{{k.type==='swiperList'}}"><index-swiper vue-id="{{'8deb4120-1-'+index+'-'+i}}" dataList="{{k.data}}" onVueInit="__l"></index-swiper></block><block a:if="{{k.type==='recommendList'}}"><recommend vue-id="{{'8deb4120-2-'+index+'-'+i}}" dataList="{{k.data}}" onVueInit="__l"></recommend><card vue-id="{{'8deb4120-3-'+index+'-'+i}}" cardTitle="猜你喜欢" onVueInit="__l"></card></block><block a:if="{{k.type==='bannerList'}}"><banner vue-id="{{'8deb4120-4-'+index+'-'+i}}" dataList="{{k.imgUrl}}" onVueInit="__l"></banner></block><block a:if="{{k.type==='iconsList'}}"><icons vue-id="{{'8deb4120-5-'+index+'-'+i}}" dataList="{{k.data}}" onVueInit="__l"></icons><card vue-id="{{'8deb4120-6-'+index+'-'+i}}" cardTitle="热销爆品" onVueInit="__l"></card></block><block a:if="{{k.type==='hotList'}}"><hot vue-id="{{'8deb4120-7-'+index+'-'+i}}" dataList="{{k.data}}" onVueInit="__l"></hot><card vue-id="{{'8deb4120-8-'+index+'-'+i}}" cardTitle="推荐商品" onVueInit="__l"></card></block><block a:if="{{k.type==='shopList'}}"><shop vue-id="{{'8deb4120-9-'+index+'-'+i}}" dataList="{{k.data}}" onVueInit="__l"></shop><card vue-id="{{'8deb4120-10-'+index+'-'+i}}" cardTitle="为您推荐" onVueInit="__l"></card></block><block a:if="{{k.type==='commodityList'}}"><commodity-list vue-id="{{'8deb4120-11-'+index+'-'+i}}" dataList="{{k.data}}" onVueInit="__l"></commodity-list></block></block></block></block><block a:else><view>暂无数据</view></block><view class="load-text f-color">{{''+item.loadText+''}}</view></scroll-view></swiper-item></swiper></view>