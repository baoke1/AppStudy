<view><view class="commodity" style="{{('flex-wrap:'+wrap+';')}}"><view class="commodity-item" style="{{('width:'+itemW+';')}}" a:for="{{dataList}}" a:for-item="item" a:for-index="index" a:key="index"><image class="commodity-img" style="{{('height:'+bigH+';')}}" src="{{item.imgurl}}"></image><view class="commodity-context"><text class="commodity-name" style="{{('font-size:'+nameSize+';')}}">{{''+item.name+''}}</text><view><text class="pprice">{{"￥"+item.pprice}}</text><text class="oprice">{{"￥"+item.oprice}}</text></view><text class="discount">{{item.discount+"折"}}</text></view></view></view></view>